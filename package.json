{
  "name": "@cdktf/aws-cdk",
  "description": "Adapter for using AWS CDK constructs in Terraform CDK (cdktf) projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/hashicorp/cdktf-aws-cdk.git"
  },
  "scripts": {
    "clobber": "npx projen clobber",
    "compile": "jsii --silence-warnings=reserved-word",
    "test:compile": "npx projen test:compile",
    "test": "jest",
    "build": "npx projen build",
    "test:watch": "npx projen test:watch",
    "test:update": "npx projen test:update",
    "bump": "npx projen bump",
    "unbump": "npx projen unbump",
    "publish:github": "npx projen publish:github",
    "upgrade": "npx projen upgrade",
    "upgrade-projen": "npx projen upgrade-projen",
    "watch": "npx projen watch",
    "package": "npx projen package",
    "compat": "npx projen compat",
    "publish:npm": "npx projen publish:npm",
    "publish:pypi": "npx projen publish:pypi",
    "publish:nuget": "npx projen publish:nuget",
    "docgen": "npx projen docgen",
    "default": "npx projen default",
    "release": "npx projen release",
    "projen": "npx projen",
    "fetch:types": "mkdir -p src/awscc && rm -rf ./src/awscc/* && node ./scripts/update-supported-types.js",
    "fetch": "mkdir -p src/aws && rm -rf ./src/aws/* && cdktf get && cp -R .gen/providers/aws/* ./src/aws/",
    "commit": "git add -A && git commit -am \"Update provider\" || echo \"No changes to commit\"",
    "should-release": "! git diff --exit-code v$(cat version.json | jq -r '.version') ./src ./package.json",
    "prebump": "yarn fetch && yarn compile && yarn run commit && yarn run should-release",
    "build-provider": "yarn fetch && yarn compile && yarn docgen"
  },
  "author": {
    "name": "HashiCorp",
    "url": "https://hashicorp.com",
    "organization": true
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.36.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.19.0",
    "aws-cdk-lib": "^2.0.0-rc.17",
    "cdktf": "0.6.4-pre.367",
    "cdktf-cli": "0.6.4-pre.367",
    "constructs": "^10.0.0",
    "jest": "^27.2.5",
    "jest-junit": "^12",
    "jsii": "^1.39.0",
    "jsii-diff": "^1.39.0",
    "jsii-docgen": "^1.8.110",
    "jsii-pacmak": "^1.39.0",
    "npm-check-updates": "^11",
    "projen": "^0.28.28",
    "standard-version": "^9",
    "ts-jest": "^27.0.5",
    "ts-node": "^9",
    "typescript": "^3.9.10"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0-rc.17",
    "cdktf": "0.6.4-pre.367",
    "constructs": "^10.0.0"
  },
  "dependencies": {
    "cdktf-cli": "0.6.4-pre.367",
    "change-case": "^4.1.1",
    "codemaker": "^1.34.0",
    "debug": "^4.3.2"
  },
  "bundledDependencies": [
    "cdktf-cli",
    "change-case",
    "codemaker",
    "debug"
  ],
  "keywords": [
    "aws",
    "aws-cdk",
    "cdk",
    "cdktf",
    "terraform"
  ],
  "engines": {
    "node": ">= 12.19.0"
  },
  "main": "lib/index.js",
  "license": "MPL-2.0",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "jest": {
    "setupFilesAfterEnv": [
      "./setupJest.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/examples",
      ".yalc",
      ".+\\.d\\.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/examples",
      ".yalc",
      ".+\\.d\\.ts"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  },
  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdktf-aws-cdk",
        "module": "cdktf_aws_cdk"
      },
      "dotnet": {
        "namespace": "HashiCorp.Cdktf.AwsCdk",
        "packageId": "HashiCorp.Cdktf.AwsCdk"
      }
    },
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.js and run \"npx projen\"."
}